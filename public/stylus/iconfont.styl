@font-face {
  font-family: 'varimJarim';
  src:url('fonts/varimJarim.eot');
  src:url('fonts/varimJarim.eot?#iefix') format('embedded-opentype'),
    url('fonts/varimJarim.ttf') format('truetype'),
    url('fonts/varimJarim.woff') format('woff'),
    url('fonts/varimJarim.svg#varimJarim') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  font-family: 'varimJarim';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

